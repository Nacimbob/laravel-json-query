{
    "name": "canedoc/json-query-builder",
    "license": "MIT",
    "homepage": "https://github.com/canedoc/laravel_json_query",
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "nunomaduro/collision": "^5.10"
    },
    "autoload": {
        "psr-4": {
            "QueryJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueryJson\\Test\\":  "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QueryJson\\QueryJsonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
